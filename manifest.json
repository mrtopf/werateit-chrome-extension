{
  "name": "We Rate It",
  "version": "0.0.6",
  "description": "Ein Experiment zur kollaborativen Altersbewertung von Websites.",
  "background_page": "background.html",
  "update_url": "http://werateit.de/extensions/chrome/update.xml",
  "icons" : {
    "16" : "icons/icon16.png",
    "48" : "icons/icon48.png",
    "128" : "icons/icon128.png"
  },
  "page_action": {
    "default_icon": "icons/no.png",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "history",
    "http://backend.werateit.de/"
  ]
}

